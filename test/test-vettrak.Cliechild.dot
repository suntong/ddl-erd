  "Child" [
    label=<
    <TABLE BORDER="0" CELLBORDER="1" CELLSPACING="0" BGCOLOR="lightyellow">
      <TR><TD COLSPAN="2" BGCOLOR="lightblue"><B>Child</B></TD></TR>
      <TR><TD COLSPAN="2" ALIGN="LEFT"><I>Referenced By Other Tables As Key:</I></TD></TR>
      <TR><TD ALIGN="LEFT">CHIL_ID</TD><TD ALIGN="LEFT">(Referenced)</TD></TR>
    </TABLE>
    >
  ];

  "Cliechild" -> "Child" [label=" Fk_Cliechild_Child "];
  "Cliechild" -> "Client" [label=" Fk_Cliechild_Parent "];
}
